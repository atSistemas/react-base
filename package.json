{
  "name": "ReactBase",
  "version": "1.1.1",
  "description": "React Base Isomorphic Redux",
  "author": "AT Sistemas",
  "main": "client/client.js",
  "scripts": {
    "logo": "node ./src/base/conf/.react-base/logo.js",
    "webpack": "node_modules/webpack/bin/webpack.js",
    "clean": "rimraf dist/ && npm run generate:dll",
    "clean:files": "rimraf dist/app.js",
    "generate": "npm run logo && yo react-base",
    "generate:component": "npm run logo && yo react-base --option=2",
    "generate:container": "npm run logo && yo react-base --option=1",
    "regenerate:modelIndex": "node ./src/base/shared/Regenerate.js --model",
    "regenerate:routes": "node ./src/base/shared/Regenerate.js --routes",
    "regenerate:reducerIndex": "node ./src/base/shared/Regenerate.js --reducer",
    "regenerate": "npm run logo && node ./src/base/shared/Regenerate.js --all",
    "generate:dll": "cross-env NODE_ENV=dll webpack --config webpack/webpack.dll.config.babel.js",
    "build": "cross-env NODE_ENV=development webpack --config webpack/index.babel.js",
    "build:prod": "npm run clean && cross-env NODE_ENV=production webpack --config webpack/index.babel.js",
    "lint": "npm run logo && node node_modules/eslint/bin/eslint.js src --ignore-pattern **/*.spec.js  --ext .js --ext .jsx",
    "lint:css": "npm run logo && stylelint \"src/**/*.css\" --config .stylelintrc",
    "lint:all": "npm run lint && npm run lint:css",
    "test": "npm run logo && mocha --compilers js:babel-core/register --recursive \"./src/**/*.spec.js\" --require ignore-styles \"./src/base/shared/JsDomSetup.js\"",
    "test:coverage": "nyc npm test && nyc report --reporter=lcov",
    "start": "npm run logo && cross-env NODE_ENV=development node server/",
    "start:prod": "npm run build:prod && cross-env NODE_ENV=production node server/",
    "yo": "npm link src/base/conf/.react-base/templates/generator-react-base",
    "postYeomanGenerator": "npm run regenerate && npm run start",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "postinstall": "npm run generate:dll"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "compression": "^1.7.4",
    "express": "^4.16.4",
    "helmet": "^3.8.1",
    "react": "15.6.1",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "react-router": "3.0.5",
    "redux": "3.7.0",
    "redux-req-middleware": "^1.0.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-webpack-alias": "2.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "^6.26.0",
    "chai": "3.5.0",
    "classnames": "2.2.5",
    "copy-webpack-plugin": "4.0.1",
    "coveralls": "2.13.1",
    "create-reduxreducer": "^1.0.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "css-modules-require-hook": "^4.2.3",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "2.1.0",
    "generator-webapp": "^2.1.0",
    "google-map-react": "0.24.0",
    "ignore-styles": "5.0.1",
    "immutable": "3.8.1",
    "isomorphic-fetch": "2.2.1",
    "istanbul": "0.4.5",
    "jsdom": "9.9.1",
    "mocha": "^3.4.2",
    "nyc": "10.3.0",
    "postcss-browser-reporter": "0.5.0",
    "postcss-cssnext": "2.10.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.3.3",
    "postcss-modules-extract-imports": "1.0.1",
    "postcss-nested": "1.0.1",
    "postcss-url": "6.0.4",
    "precss": "^2.0.0",
    "progress-bar-webpack-plugin": "1.9.3",
    "react-addons-linked-state-mixin": "15.5.2",
    "react-controllables": "0.6.0",
    "react-ink": "6.2.0",
    "react-pure-render": "1.0.2",
    "react-test-renderer": "^15.6.1",
    "redux-logger": "3.0.1",
    "rimraf": "2.6.1",
    "sinon": "2.2.0",
    "style-loader": "0.17.0",
    "stylelint": "7.10.1",
    "supports-color": "3.2.3",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "^2.18.0",
    "yeoman-option-or-prompt": "^1.0.2",
    "yo": "1.8.5"
  },
  "nyc": {
    "exclude": [
      "webpack/**",
      "**/*.spec.js",
      "src/base/wp-plugins/**"
    ],
    "extension": [
      ".jsx",
      ".es6",
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atsistemas/react-base"
  },
  "bugs": {
    "url": "https://github.com/atsistemas/react-base/issues"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "starter",
    "starterKit",
    "seed",
    "isomorphic",
    "universal",
    "postcss",
    "css-modules",
    "webpack",
    "enzyme",
    "testing",
    "yeoman",
    "istambul"
  ]
}
